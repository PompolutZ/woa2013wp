
<Application x:Class="WackenOpenAir.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:phone="clr-namespace:Microsoft.Phone.Controls;assembly=Microsoft.Phone"
             xmlns:shell="clr-namespace:Microsoft.Phone.Shell;assembly=Microsoft.Phone"
             xmlns:utilities="clr-namespace:WackenOpenAir.Utilities"
             xmlns:vm="clr-namespace:WackenOpenAir.ViewModel"
             xmlns:wackenOpenAir="clr-namespace:WackenOpenAir"
             mc:Ignorable="d">
    <!--  Application Resources  -->
    <Application.Resources>
        <wackenOpenAir:WackenBootstrapper x:Key="bootstrapper" />
        <utilities:LocalizedStrings x:Key="LocalizedStrings" />
    </Application.Resources>
</Application>